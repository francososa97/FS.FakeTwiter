<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <ExcludeByFile>
            **/Program.cs
            **/StartupPlaceholder.cs
            **/*Dto.cs
            **/*Exception.cs
            **/Migrations/*
          </ExcludeByFile>
          <ExcludeByAttribute>
            CompilerGeneratedAttribute
            ExcludeFromCodeCoverage
            GeneratedCodeAttribute
          </ExcludeByAttribute>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
